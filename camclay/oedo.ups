<?xml version="1.0" encoding="iso-8859-1"?>
<Uintah_specification>

   <Meta>
      <title>1D</title>
   </Meta>

   <SimulationComponent type="mpm" />

   <Time>
       <maxTime>3</maxTime>
       <initTime>0.0</initTime>
       <delt_min>1e-7</delt_min>
       <delt_max>1e-3</delt_max>
       <max_Timesteps>300000000</max_Timesteps>
       <timestep_multiplier>0.4</timestep_multiplier>
   </Time>
   <DataArchiver>
       <filebase>camclay-wet.uda</filebase>
       <outputInterval>1.0e-2</outputInterval>
       <save label = "p.particleID"/>
       <save label = "p.x"/>
       <save label = "p.volume"/>
       <save label = "p.stress"/>
       <save label = "p.temperature"/>
       <save label = "p.velocity" />

      <save label = "p.externalforce"/>
       <save label = "p.deformationMeasure" />
        <save label="p.porepressure" /> 
       <save label = "g.mass"/>
   </DataArchiver>

    <MPM>
       <time_integrator>explicit</time_integrator>
       <interpolator>linear</interpolator>
       <artificial_damping_coeff>0.0</artificial_damping_coeff>
      <use_load_curves>true</use_load_curves>
      <axisymmetric>false</axisymmetric>
          <coupled_flow_analysis>true</coupled_flow_analysis>
    </MPM>

    <PhysicalConstants>
       <gravity>[0,0,0]</gravity>   
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>
            <material>
               <density>1700</density>
               <water_density>1000</water_density>
               <permeability>1e-4</permeability>
               <porosity>0.7</porosity>
               <constitutive_model type="camclay_imp">
                   <nu> 0.3</nu>
                   <e0> 2.36</e0>
                   <kappa> 0.05</kappa>
                   <lambda> 0.3 </lambda>
                   <M> 1 </M>
                   <p_ini> 10000 </p_ini>
                   <OCR>4</OCR>
               </constitutive_model> 
               <thermal_conductivity>1</thermal_conductivity> 
               <specific_heat>1</specific_heat> 
               <geom_object> 
                   <box label="soil"> 
                      <min>[0,0,0]</min> 
                      <max>[1,1,0.1]</max> 
                   </box> 
                   <res>[1,1,1]</res> 
                   <velocity>[0.0,0.0,0.0]</velocity> 
                   <temperature>12</temperature> 
               </geom_object> 
            </material> 
       </MPM>
    </MaterialProperties>

    <PhysicalBC>
      <MPM>
      <pressure>
         <geom_object>
           <box>
             <min>           [0.0,0.9999,0.0]   </min>
             <max>           [1.0,1.01,0.1]   </max>
           </box>
         </geom_object>
         <load_curve>
           <id>1</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 1 </time>
             <load> -1e4 </load>
           </time_point>
           <time_point>
             <time> 10 </time>
             <load> -1e4 </load>
           </time_point>
         </load_curve>
       </pressure>
      </MPM>
    </PhysicalBC>

    <Grid>
            <BoundaryConditions>
             <Face side = "x-">
          <BCType id = "all" var = "symmetry" label = "Symmetric">
          </BCType>
       </Face>
       <Face side = "x+">
          <BCType id = "all" var = "symmetry" label = "Symmetric">
          </BCType>
       </Face>
       <Face side = "y-">
          <BCType id = "all" var = "symmetry" label = "Symmetric">
          </BCType>
       </Face>
       <Face side = "y+">
          <BCType id = "all" var = "symmetry" label = "Symmetric">
          </BCType>
       </Face>
       <Face side = "z-">
          <BCType id = "all" var = "symmetry" label = "Symmetric">
          </BCType>
       </Face>
       <Face side = "z+">
          <BCType id = "all" var = "symmetry" label = "Symmetric">
          </BCType>
       </Face>
       </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>[0, 0, 0]   </lower>
              <upper>[1, 1.2, 0.1]</upper>
              <extraCells>[0,0,0]      </extraCells>
              <patches>[1,1,1]         </patches>
           <resolution>[6, 48, 1]</resolution>
           </Box>
       </Level>

    </Grid>

</Uintah_specification>

